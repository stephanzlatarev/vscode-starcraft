{
  "name": "vscode-starcraft",
  "description": "Visual Studio Code extension for StarCraft II bots",
  "version": "1.0.3",
  "publisher": "stephanzlatarev",
  "repository": "https://github.com/stephanzlatarev/vscode-starcraft",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "starcraft.startGame",
        "title": "StarCraft II: Start game"
      }
    ]
  },
  "scripts": {
    "package": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "npm run package"
  },
  "dependencies": {
    "@node-sc2/proto": "^0.6.0-alpha.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "file-loader": "^6.2.0",
    "vscode": "*",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
